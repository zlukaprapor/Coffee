<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<?import java.net.URL?>
<StackPane  prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.edu.coffee.MenuController">
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets >
    <HBox spacing="120" alignment="TOP_CENTER" styleClass="background">
        <Button fx:id="menuButton" mnemonicParsing="false" styleClass="button" text="Меню" />
        <Button fx:id="ordersButton" mnemonicParsing="false" styleClass="button" text="Замовлення"/>
        <Button fx:id="queueButton" mnemonicParsing="false" styleClass="button" text="Черга"/>
        <Button fx:id="optionsButton" mnemonicParsing="false" styleClass="button" text="Опції" />
    </HBox>
</StackPane>

